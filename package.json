{
  "name": "jack",
  "displayName": "lazyJack",
  "description": "lets go",
  "version": "0.0.19",
  "publisher": "jackFan",
  "icon": "image/lazy_man.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "flutter",
    "bloc",
    "freezed"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lazy-sidebar",
          "title": "lazy-sidebar",
          "icon": "/image/rocket.svg"
        }
      ]
    },    
    "viewsWelcome": [
			{
					"view": "fast-repo-gui",
					"contents": "Open github Repository \n[Open](command:command_open_github_repo)\n[Actions](command:command_open_github_actions)\n[Wiki](command:command_open_github_wiki)\nOpen local sourcetree\n[Open](command:command_open_sourcetree_local_repo)"
			}
        ],
    "views": {
      "lazy-sidebar": [
        {
          "id": "fast-repo-gui",
          "name": "fast-repo-gui"
        },
        {
          "id": "flutter-lazy-cmd",
          "name": "flutter-lazy-cmd"
        },
        {
          "id": "build_runner-lazy-cmd",
          "name": "build_runner-lazy-cmd"
        },
        {
          "id": "git-lazy-cmd",
          "name": "git-lazy-cmd"
        },
        {
          "id": "npm-lazy-cmd",
          "name": "npm-lazy-cmd"
        },
        {
          "id": "vscode-extension-lazy-cmd",
          "name": "vscode-extension-lazy-cmd"
        }
      ]
    },
    "commands": [
      {
        "category": "lazy Jack",
        "command": "command_dart_generate_getter_setter",
        "title": "Generate getter setter"
      },
      {
        "category": "lazy Jack",
        "command": "dart.import.freezed",
        "title": "Import freezed part"
      },
      {
        "category": "lazy Jack",
        "command": "command_create_freezed_class_by_base_fileName",
        "title": "Create freezed class"
      },
      {
        "category": "lazy Jack",
        "command": "command_create_freezed_class_with_state_by_base_fileName",
        "title": "Create freezed class with state"
      },
      {
        "category": "lazy Jack",
        "command": "command_add_freezed_state_by_base_fileName",
        "title": "Add new freezed State"
      },
      {
        "category": "lazy Jack",
        "command": "command_create_from_json",
        "title": "Crate FromJson by file name"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus && resourceLangId == dart",
          "command": "command_dart_generate_getter_setter",
          "group": "0_jack_dart@1"
        },
        {
          "when": "editorFocus && resourceLangId == dart",
          "command": "dart.import.freezed",
          "group": "0_jack_dart@2"
        },
        {
          "when": "editorFocus && resourceLangId == dart",
          "command": "command_create_freezed_class_by_base_fileName",
          "group": "0_jack_dart@3"
        },
        {
          "when": "editorFocus && resourceLangId == dart",
          "command": "command_create_freezed_class_with_state_by_base_fileName",
          "group": "0_jack_dart@4"
        },
        {
          "when": "editorFocus && resourceLangId == dart",
          "command": "command_create_from_json",
          "group": "0_jack_dart@5"
        },
        {
          "when": "editorFocus && resourceLangId == dart",
          "command": "command_add_freezed_state_by_base_fileName",
          "group": "0_jack_dart@6"
        }
      ]
    },
    "snippets": [
      {
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "dart",
        "path": "./snippets/dart_freezed.json"
      },
      {
        "language": "dart",
        "path": "./snippets/dart_unit_test.json"
      },
      {
        "language": "dart",
        "path": "./snippets/dart.json"
      }
    ]
  },
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jack-fan1991/vscode.git"
  },
  "extensionPack": [
    "FelixAngelov.bloc"
  ],
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4",
    "vscode": "^1.1.34"
  }
}
